{
  "metadata": [
    {
      "src": "src/FubarDev.WebDavServer.AspNetCore/*.csproj",
      "dest": "temp/api/netstandard1.6",
      "properties": {
          "TargetFramework": "netstandard1.6"
      }
    },
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ],
          "exclude": [
            "src/FubarDev.WebDavServer.AspNetCore/*.csproj"
          ]
        }
      ],
      "dest": "temp/api/netstandard1.3",
      "properties": {
          "TargetFramework": "netstandard1.3"
      }
    },
    {
      "src": "src/**/*.csproj",
      "dest": "temp/api/net451",
      "properties": {
          "TargetFramework": "net451"
      }
    }
  ],
  "merge": {
    "content": [
      {
        "files": "*.yml",
        "src": "temp/api/netstandard1.3"
      },
      {
        "files": "*.yml",
        "src": "temp/api/netstandard1.6"
      },
      {
        "files": "*.yml",
        "src": "temp/api/net451"
      }
    ],
    "fileMetadata": {
      "platform": {
        "temp/api/netstandard1.3/*.yml": [
          "netstandard1.3",
          "netstandard1.6"
        ],
        "temp/api/netstandard1.6/*.yml": [
          "netstandard1.6"
        ],
        "temp/api/net451/*.yml": [
          "net451"
        ]
      }
    },
    "dest": "api"
  },
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadata": {
      "_gitContribute": {
        "repo": "https://github.com/FubarDevelopment/WebDavServer",
        "branch": "master"
      },
      "_gitUrlPattern": "github",
      "_appFooter": "<span>Copyright Â© 2016-2017 Fubar Development Junker<br>Generated by <strong>DocFX</strong></span>"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "tools/docfx-templates/default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "markdownEngineName": "dfm-latest",
    "xref": ["tools/xref-zips/msdn.4.5.2.zip", "tools/xref-zips/namespaces.4.5.2.zip"]
  }
}